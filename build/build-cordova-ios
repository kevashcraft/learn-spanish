/bin/build-app
rm -rf /cordova-ios/*
cd /cordova-ios
cordova create spanish com.kevapps.spanish-photo-flash "Spanish"
cd spanish
cordova platform add ios
cordova plugin add cordova-plugin-file
cordova plugin add cordova-plugin-device
cp /res/icon.png ./
cp /res/splash.png ./
cordova-icon
cordova-splash
rm -rf www
mv /app/dist www
